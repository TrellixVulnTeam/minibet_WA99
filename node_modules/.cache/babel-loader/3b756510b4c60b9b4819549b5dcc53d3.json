n}\n\n.greeting-wrap div.content {\n    color: #eee;\n    display: flex;\n    flex-direction: column;\n    text-align: left;\n    margin-left: 0.5rem;\n}\n\n.greeting-wrap div.content div:nth-child(1) {\n    font-size: 1.125rem;\n    letter-spacing: -0.01875rem;\n}\n\n.greeting-wrap div.content div:nth-child(2) {\n    font-size: 1.5rem;\n    letter-spacing: -0.0375rem;\n}\n\n.greeting-icon{\n  height: 3.25rem;\n  width: 3.25rem;\n  background-image: url(\"../assets/icons/outline/greeting/morning.png\");\n  background-size: contain;\n  background-repeat: no-repeat;\n}\n@media only screen and (min-width: 600px) {\n    .greeting-wrap div.content {\n        flex-direction: row;\n    }\n\n    .greeting-wrap div.content div:nth-child(2) {\n        font-size: 1.125rem;\n        margin-left: 0.5rem;\n    }\n\n    .greeting-icon{\n      height: 1.625rem;\n      width: 1.625rem;\n      background-image: url(\"../assets/icons/outline/greeting/morning-sm.png\");\n      background-size: contain;\n    }\n}\n\n</style>\n\n<template>\n\n<div class=\"greeting-wrap\" style=\"margin-top: 1.25rem\">\n    <div class=\"greeting-icon\"></div>\n    <div class=\"content\">\n        <div>{{greetings[getTimeOfDay()]}}</div>\n        <div>Enzo</div>\n    </div>\n</div>\n\n</template>\n\n<script>\n\nexport default {\n    name: 'Greeting',\n    data() { // variable declarations\n        return {\n            greetings: [\"Good Morning\", \"Good Afternoon\", \"Good Evening\"]\n        }\n    },\n    methods: {\n        getTimeOfDay() {\n            var currentDate = new Date();\n            var currentHour = currentDate.getHours();\n            var timeOfDay = 0;\n\n            switch (true) {\n                case currentHour >= 0 && currentHour <= 11:\n                    timeOfDay = 0;\n                    break;\n                case currentHour >= 12 && currentHour <= 17:\n                    timeOfDay = 1;\n                    break;\n                case currentHour >= 18 && currentHour <= 23:\n                    timeOfDay = 2;\n                    break;\n                default:\n                    break;\n            }\n\n            return timeOfDay;\n        }\n    },\n    mounted() {\n\n    }\n}\n\n</script>\n"],"sourceRoot":""}]}